@model IEnumerable<NVTLongBTH2.Models.Student>
<p>
    <a class="btn btn-success" asp-action="Create">Create a new Student</a>
    <a class="btn btn-success" asp-action="Edit">Sửa thông tin sinh viên</a>
    <a class="btn btn-success" asp-action="Delete">Xóa thông tin sinh viên</a>
</p>
<table class="table table-all">
    <tr>
        <th>StudentID</th>
        <th>StudentName</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.StudentID)</td>
            <td>@Html.DisplayFor(m => item.StudentName)</td>
        </tr>
    }
</table>