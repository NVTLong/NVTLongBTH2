@model IEnumerable<NVTLongBTH2.Models.Employee>
@{
    ViewData["Title"] = "Index";
}
<h1>Index</h1>
<p>
    <a asp-action="Upload">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmpName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model){
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EmpName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    <a asp-action="Edit"asp-route-itemid="@item.EmpID">Edit</a>
                    <a asp-action="Details"asp-route-itemid="@item.EmpID">Details</a>
                    <a asp-action="Delete"asp-route-itemid="@item.EmpID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>